FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         2000;

deltaT          1;

writeControl    timeStep;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    stressAccumulation
    {
        type stressAccumulation;
        libs ("libstressAccumulationFunctionObject.so");
        outletPatch outlet;

        // Damage properties
        sigma0    [1 -1 -2 0 0 0 0] 100;
        k         -1.0;
        r         5.0;

        // Non-orthogonal correctors for transport equations
        nNonOrthoCorr 3;
    }
}


// ************************************************************************* //
